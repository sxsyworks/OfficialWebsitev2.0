{
  "private": true,
  "scripts": {
    "start": "max dev",
    "build": "max build",
    "postinstall": "max setup",
    "analyze": "cross-env ANALYZE=1 umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^5.11.3",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "lodash": "^4.17.21",
    "react": "17.x",
    "react-dom": "17.x",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/max": "^4.0.30",
    "@umijs/test": "^3.5.23",
    "animate.css": "^4.1.1",
    "echarts": "^5.3.2",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react-bmapgl": "^0.2.7",
    "resize-observer-polyfill": "^1.5.1",
    "three": "^0.145.0",
    "typescript": "^5.8.3",
    "vconsole": "^3.14.6",
    "wowjs": "^1.1.3",
    "yorkie": "^2.0.0"
  }
}
